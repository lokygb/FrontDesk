<?xml version="1.0" ?>
<xs:schema 
	targetNamespace="urn:frontdesk-result"
	xmlns="urn:frontdesk-result" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	attributeFormDefault="qualified"
	elementFormDefault="qualified">
	
	<xs:element name="ResultSubj">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Comment" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
						        <xs:element name="Type" type="xs:string" maxOccurs="1" minOccurs="1" />
							<xs:element name="Points" type="xs:string" maxOccurs="1" minOccurs="1" />							
							<xs:element name="Message" type="xs:string" maxOccurs="1" minOccurs="1" />
							<xs:element name="File" type="xs:string" maxOccurs="1" minOccurs="0" />
							<xs:element name="Line" type="xs:string" maxOccurs="1" minOccurs="0" />	
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>